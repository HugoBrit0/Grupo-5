@model IEnumerable<WebApplication1.Models.Admin>

<!DOCTYPE html>
<html lang="pt-PT">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Lista de Administradores</title>
</head>
<body>
    <h1>Lista de Administradores</h1>

    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nome</th>
                <th>Email</th>
                <!-- Adiciona mais colunas conforme necessário -->
            </tr>
        </thead>
        <tbody>
            @foreach (var admin in Model)
            {
                <tr>
                    <td>@admin.Id</td> <!-- Ajusta conforme as propriedades do modelo Admin -->
                    <td>@admin.Nome</td> <!-- Ajusta conforme as propriedades do modelo Admin -->
                    <td>@admin.Email</td> <!-- Ajusta conforme as propriedades do modelo Admin -->
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
